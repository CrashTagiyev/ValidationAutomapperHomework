@model List<User>

@{
	ViewData["Title"] = "Index";
}

<h1>Users</h1>


<table class="table">
	<thead>
		<tr>
			<th>
				ID
			</th>
			<th>
				Username
			</th>
			<th>
				Email
			</th>
			<th>
				Address
			</th>
			<th>
				City
			</th>
			<th>
				Region
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@item.Id
				</td>
				<td>
					@item.Usernaame
				</td>
				<td>
					@item.Email
				</td>
				<td>
					@item.Address
				</td>
				<td>
					@item.City
				</td>
				<td>
					@item.Region
				</td>
			</tr>
		}
	</tbody>
</table>
